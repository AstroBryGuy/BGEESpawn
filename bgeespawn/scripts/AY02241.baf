IF
  Global("AYSpawnRandomBeenSetAR02241","AR0224",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR02241","AR0224",1)
    SetGlobal("AYSpawnResetAR02241","AR0224",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR02241_0","AR0224",0)
  Global("AYSpawnRandomBeenSetAR02241","AR0224",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR02241_0","AR0224",1)
    SetGlobal("AYSpawnRandomBeenSetAR02241","AR0224",1)
END

