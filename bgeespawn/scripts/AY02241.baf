  Range([PC],60)
  LevelPartyGT(4)
  LevelPartyLT(6)
  Global("AYSpawnResetAR02241","AR0224",0)
  Global("AYSpawnRandomSetAR02241_0","AR0224",1)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnResetAR02241","AR0224",1)
    CreateCreature("SPIDPH",[1024.1942],0)  // ~Phase Spider~
    SetGlobalTimer("AYSpawnTimerAR02241","AR0224",AY_SPAWN_TIMER)
END

