IF
  Global("AYSpawnRandomBeenSetAR05140","AR0514",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR05140","AR0514",1)
    SetGlobal("AYSpawnResetAR05140","AR0514",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR05140_0","AR0514",0)
  Global("AYSpawnRandomBeenSetAR05140","AR0514",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR05140_0","AR0514",1)
    SetGlobal("AYSpawnRandomBeenSetAR05140","AR0514",1)
END

