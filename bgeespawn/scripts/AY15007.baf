IF
  Global("AYSpawnRandomBeenSetAR15007","AR1500",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR15007","AR1500",1)
    SetGlobal("AYSpawnResetAR15007","AR1500",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR15007_0","AR1500",0)
  Global("AYSpawnRandomBeenSetAR15007","AR1500",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR15007_0","AR1500",1)
    SetGlobal("AYSpawnRandomBeenSetAR15007","AR1500",1)
END

IF
  Range([PC],60)
  LevelPartyGT(1)
  LevelPartyLT(3)
  Global("AYSpawnResetAR15007","AR1500",0)
  Global("AYSpawnRandomSetAR15007_0","AR1500",1)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnResetAR15007","AR1500",1)
    CreateCreature("WOLFDI",[1795.628],0)  // ~Dire Wolf~
    SetGlobalTimer("AYSpawnTimerAR15007","AR1500",AY_SPAWN_TIMER)
END

