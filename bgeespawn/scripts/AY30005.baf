IF
  Global("AYSpawnRandomBeenSetAR30005","AR3000",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR30005","AR3000",1)
    SetGlobal("AYSpawnResetAR30005","AR3000",1)
  RESPONSE #GOSPAWN
    Continue()
END

