IF
  Global("AYSpawnRandomBeenSetAR15001","AR1500",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR15001","AR1500",1)
    SetGlobal("AYSpawnResetAR15001","AR1500",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(2,1)
  Global("AYSpawnRandomSetAR15001_1","AR1500",0)
  Global("AYSpawnRandomBeenSetAR15001","AR1500",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR15001_1","AR1500",1)
    SetGlobal("AYSpawnRandomBeenSetAR15001","AR1500",1)
END

