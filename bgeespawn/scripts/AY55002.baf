IF
  !Range([PC],60)

  !Range("GIBBER",60)
  Global("AYSpawnResetAR55002","AR5500",1)
  GlobalTimerExpired("AYSpawnTimerAR55002","AR5500")
THEN
  RESPONSE #100
    SetGlobal("AYSpawnResetAR55002","AR5500",0)
    SetGlobal("AYSpawnRandomBeenSetAR55002","AR5500",0)
    SetGlobal("AYSpawnRandomSetAR55002_0","AR5500",0)
END

