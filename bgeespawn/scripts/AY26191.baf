IF
  Global("AYSpawnRandomBeenSetAR26191","AR2619",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR26191","AR2619",1)
    SetGlobal("AYSpawnResetAR26191","AR2619",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR26191_0","AR2619",0)
  Global("AYSpawnRandomBeenSetAR26191","AR2619",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR26191_0","AR2619",1)
    SetGlobal("AYSpawnRandomBeenSetAR26191","AR2619",1)
END

IF
  Range([PC],60)
  LevelPartyLT(2)
  Global("AYSpawnResetAR26191","AR2619",0)
  Global("AYSpawnRandomSetAR26191_0","AR2619",1)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnResetAR26191","AR2619",1)
    CreateCreature("SKELET_C",[3096.1204],0)  // ~Skeleton~
    CreateCreature("SKELET_C",[3096.1204],0)  // ~Skeleton~
    SetGlobalTimer("AYSpawnTimerAR26191","AR2619",AY_SPAWN_TIMER)
END

