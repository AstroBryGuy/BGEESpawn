IF
  Global("AYSpawnRandomBeenSetAR47006","AR4700",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR47006","AR4700",1)
    SetGlobal("AYSpawnResetAR47006","AR4700",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR47006_0","AR4700",0)
  Global("AYSpawnRandomBeenSetAR47006","AR4700",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR47006_0","AR4700",1)
    SetGlobal("AYSpawnRandomBeenSetAR47006","AR4700",1)
END

