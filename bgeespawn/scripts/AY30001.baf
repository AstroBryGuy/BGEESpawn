IF
  Global("AYSpawnRandomBeenSetAR30001","AR3000",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR30001","AR3000",1)
    SetGlobal("AYSpawnResetAR30001","AR3000",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR30001_0","AR3000",0)
  Global("AYSpawnRandomBeenSetAR30001","AR3000",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR30001_0","AR3000",1)
    SetGlobal("AYSpawnRandomBeenSetAR30001","AR3000",1)
END

IF
  Range([PC],60)
  LevelPartyGT(1)
  LevelPartyLT(3)
  Global("AYSpawnResetAR30001","AR3000",0)
  Global("AYSpawnRandomSetAR30001_0","AR3000",1)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnResetAR30001","AR3000",1)
    CreateCreature("SPIDHU",[2496.704],0)  // ~Huge Spider~
    SetGlobalTimer("AYSpawnTimerAR30001","AR3000",AY_SPAWN_TIMER)
END

