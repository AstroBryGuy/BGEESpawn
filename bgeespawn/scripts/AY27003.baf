IF
  Global("AYSpawnRandomBeenSetAR27003","AR2700",0)
THEN
  RESPONSE #NOSPAWN
    SetGlobal("AYSpawnRandomBeenSetAR27003","AR2700",1)
    SetGlobal("AYSpawnResetAR27003","AR2700",1)
  RESPONSE #GOSPAWN
    Continue()
END

IF
  RandomNum(1,1)
  Global("AYSpawnRandomSetAR27003_0","AR2700",0)
  Global("AYSpawnRandomBeenSetAR27003","AR2700",0)
THEN
  RESPONSE #100
    SetGlobal("AYSpawnRandomSetAR27003_0","AR2700",1)
    SetGlobal("AYSpawnRandomBeenSetAR27003","AR2700",1)
END

